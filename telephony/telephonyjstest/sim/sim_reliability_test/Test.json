{
    "description": "Configuration for sim reliability Tests",
    "driver": {
        "type": "JSUnitTest",
        "test-timeout": "540000",
        "package": "com.ohos.simreliability",
        "shell-timeout": "540000",
        "native-test-timeout":"540000"
    },
    "kits": [
        {
            "test-file-name": [
                "$module.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        }
    ]
}
