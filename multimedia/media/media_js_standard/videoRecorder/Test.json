{
    "description": "Configuration for videoRecorder Tests",
    "driver": {
        "type": "JSUnitTest",
        "test-timeout": "1000000",
        "package": "ohos.acts.multimedia.video.videorecorder",
        "shell-timeout": "60000"
    },
    "kits": [
        {
            "type": "ShellKit",
            "run-command": [
                "setenforce 0"
            ],
            "teardown-command":[
                "setenforce 1"
            ]
        },
        {
            "test-file-name": [
                "VideoRecorderJsTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        }
    ]
}