{
    "description": "Configuration for audio manager Tests",
    "driver": {
        "type": "JSUnitTest",
        "test-timeout": "5500000",
        "package": "ohos.acts.multimedia.audio.audiomanager",
        "shell-timeout": "60000"
    },
    "kits": [
        {
            "test-file-name": [
                "ActsAudioManagerJsTest.hap"
            ],
            "type": "AppInstallKit",
            "run-command": [
                "chmod 777 /data/*",
                "chmod 777 /data -R"
            ],
            "cleanup-apps": true
        }
    ]
}