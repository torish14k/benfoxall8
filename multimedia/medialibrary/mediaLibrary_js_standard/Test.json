{
    "description": "Configuration for mediaLibrary Tests",
    "driver": {
        "type": "JSUnitTest",
        "test-timeout": "120000",
        "package": "ohos.acts.multimedia.mediaLibrary",
        "shell-timeout": "120000"
    },
    "kits": [
        {
            "type": "PushKit",
            "pre-push": [
            ],
            "push": [
            ]
        },
        {
            "type": "ShellKit",
            "run-command": [
                "chmod -R 777 /storage/media/*",
                "setenforce 0"
            ],
            "teardown-command":[
                "setenforce 1"
            ]
        },
        {
            "test-file-name": [
                "ActsMediaLibraryJsTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        }
    ]
}