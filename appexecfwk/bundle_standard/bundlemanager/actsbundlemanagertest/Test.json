                "bmsJstest5.hap->/data/bmsJstest5.hap",
                "bmsJstest6.hap->/data/bmsJstest6.hap",
                "bmsJstest7.hap->/data/bmsJstest7.hap",
                "bmsJstest8.hap->/data/bmsJstest8.hap",
