{
    "description": "Config for zlib test cases",
    "driver": {
        "module-name": "ZlibCppTest",
        "native-test-timeout": "120000",
        "native-test-device-path": "/data/local/tmp",
        "runtime-hint": "1s",
        "type": "CppTest"
    },
    "kits": [
        {
            "post-push" : [
                "chmod -R 777 /data/local/tmp/*"
            ],
            "push": [
                "ZlibCppTest->/data/local/tmp/ZlibCppTest"
            ],
            "type": "PushKit"
        }
    ]
}