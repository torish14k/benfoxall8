{
    "description": "Configuration for hjunit demo Tests",
    "driver": {
        "type": "JSUnitTest",
        "test-timeout": "60000",
        "package": "com.example.actsgetwantalltest",
        "shell-timeout": "60000"
    },
    "kits": [
        {
            "test-file-name": [
                "ActsGetWantAllTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "PushKit",
            "push": [
                "ActsGetWantAllTestHap.hap->/data/ActsGetWantAllTestHap.hap"
            ]
            
        }        
    ]
}
