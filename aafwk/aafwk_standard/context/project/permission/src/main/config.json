{
  "app": {
    "bundleName": "com.example.permission",
    "vendor": "example",
    "version": {
      "code": 1,
      "name": "1.0"
    },
    "apiVersion": {
      "compatible": 4,
      "target": 5,
      "releaseType": "Beta1"
    }
  },
  "deviceConfig": {},
  "module": {
    "package": "com.example.permission",
    "name": ".MyApplication",
    "defPermissions": [
      {
        "name": "ohos.permission.CAMERA",
        "grantMode": "system_grant",
        "availableScope": [],
        "label": "CAMERA permission",
        "description": "CAMERA permission in detail"
      }
    ],
    "deviceType": [
      "phone"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "permission",
      "moduleType": "feature",
      "installationFree": true
    },
    "abilities": [
      {
        "visible": true,
        "name": "com.example.permission.MainAbility",
        "icon": "$media:icon",
        "description": "$string:mainability_description",
        "label": "$string:app_name",
        "type": "page",
        "launchType": "standard"
      }
    ],
    "js": [
      {
        "pages": [
          "pages/index/index"
        ],
        "name": "default",
        "window": {
          "designWidth": 720,
          "autoDesignWidth": false
        }
      }
    ]
  }
}